{
  "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module",
      "impliedStrict": true,
      "ecmaFeatures": {
        "jsx": true,
        "impliedStrict": true,
        "globalReturn": false,
        "experimentalObjectRestSpread": true
      }
    },
    "env": {
      "browser": true,
      "node": true,
      "jest": true,
      "es6": true
    },
    "plugins": [
      "import",
      "react",
      "babel"
    ],
    "rules": {
    }
  }